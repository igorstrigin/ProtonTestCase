<Window x:Class="ProtonTestCase.Forms.EditGraphicForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="{DynamicResource form_EditGraphic}" Height="200" Width="400">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="150">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" Width="75" Text="{DynamicResource label_EditGraphic}" Margin="5"/>
            <TextBox Name="tbEditGraphic" MinWidth="200" Margin="5" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"></TextBox>
            <Button Name="btnEdit" Content="{DynamicResource btn_Send}" Height="45" Width="80" Click="btnEdit_Click" Margin="5"></Button>
        </StackPanel>
    </Grid>
</Window>
