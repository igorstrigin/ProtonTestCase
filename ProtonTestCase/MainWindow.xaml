<Window x:Class="ProtonTestCase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProtonTestCase"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem Header="{DynamicResource m_File}"></MenuItem>
            <MenuItem Header="{DynamicResource m_Language}" Name="menuLanguage">
                <MenuItem Header="{DynamicResource m_ru_Ru}"/>
                <MenuItem Header="{DynamicResource m_en_US}"/>
            </MenuItem>
            <MenuItem Header="{DynamicResource m_Graphics}">
                <MenuItem Name="mGraphicsGenerateRandom" Header="{DynamicResource m_Graphics_Generate}"/>
                <MenuItem Name="mGraphicsClear" Header="{DynamicResource m_Graphics_Clear}"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
